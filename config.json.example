# Configuration file for YeuMoney Pro
# Copy this to config.json and fill in your values

{
  "app_config": {
    "name": "YeuMoney Pro",
    "version": "3.0.0",
    "environment": "production",
    "debug": false
  },
  "bot_config": {
    "token": "YOUR_TELEGRAM_BOT_TOKEN_HERE",
    "admin_users": [123456789],
    "key_lifetime_hours": 24,
    "max_keys_per_user": 1,
    "rate_limit": {
      "messages_per_minute": 20,
      "commands_per_hour": 100
    }
  },
  "web_config": {
